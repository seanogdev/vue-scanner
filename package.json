{
  "name": "vue-scanner",
  "version": "1.0.0",
  "description": "A tool to scan vue files and generate a json file with all the components and their props",
  "bin": {
    "vue-scanner": "/dist/cli.mjs"
  },
  "main": "/dist/api.mjs",
  "type": "module",
  "scripts": {
    "build": "tsup lib/api.ts lib/cli.ts --format esm,cjs --dts",
    "dev": "pnpm build --watch",
    "lint": "eslint --ext .ts .",
    "test": "vitest",
    "test-example": "node example/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vue/compiler-dom": "^3.3.11",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "glob": "^10.3.10",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "commander": "^11.1.0"
  }
}
